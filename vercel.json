{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
   "rewrites": [
    {
        "source": "/upload/(.*)",
        "destination": "/upload/$1"
    },
    {
        "source": "/(.*)",
        "destination": "/"
    }
]

}
